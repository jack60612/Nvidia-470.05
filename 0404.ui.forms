			<form name="InstallRemoveProgress" base="!InstallRemoveProgressTemplate" locale="0x0404"/>
			<form name="PreRebootInstall" base="!PreRebootInstallTemplate" locale="0x0404"/>
			<form name="InstallProgress" base="!InstallProgressTemplate" locale="0x0404"/>
			<form name="PackageDownloadProgress" base="PackageDownloadProgressTemplate" locale="0x0404"/>
			<form name="ProcessDownloadedPackage" base="ProcessDownloadedPackageTemplate" locale="0x0404"/>
		</else>
	</if>
	<form name="InstallSelectInstallationPath" base="!SelectInstallationPathTemplate" locale="0x0404"/>
