	<description/>
	<interfaces>
		<interface name="stereoInterface" version="${{stereoInterfaceVersion}}"/>
	</interfaces>
	<constraints>
		<if filter="!OEMPreloadApps">
			<if filter="isDeclarative">
				<if filter="maxRS3">
					<custom name="InfPlatformCheck" level="skip" text="${{failedDeclarativeCheck}}"/>
					<else>
						<custom name="InfPlatformCheck" level="skip" text="${{failedInfPlatformCheck}}"/>
					</else>
				</if>
			</if>
			<if filter="!isDeclarative">
				<custom name="InfPlatformCheck" level="skip" text="${{failedInfPlatformCheck}}"/>
			</if>
			<device name="HardwarePresentCheck" level="skip" text="${{failedHardwarePresentCheck}}"/>
			<custom name="ExternalGraphicsDowngradeCheck" text="${{ExternalGraphicsDowngradeCheckWarning}}" level="warn"/>
			<custom name="AllowInstallCheck" text="${{failedHardwarePresentCheck}} ${{BlockFourPartInstallKBLink}}" level="skip"/>
			<custom name="BlockTH1Install" text="${{failedInfPlatformCheck}}" level="skip"/>
			<custom name="CriticalBsodIssueCheck" text="${{ErrorInstallerNeedReboot}}" level="skip"/>
			<if filter="win7">
				<custom name="SHA2PatchCheckWin7" text="${{BlockWin7InstallMessage}}" level="skip"/>
			</if>
		</if>
	</constraints>
	<dependencies>
		<package type="uninstalls" package="Display.3DVision"/>
		<package type="uninstalls" package="Display.NVIRUSB"/>
		<package type="uninstalls" package="DisplayDriverAnalyzer"/>
		<package type="uninstalls" package="NGXCore"/>
	</dependencies>
	<extensions>
		<extension file="DisplayDriverExt.DLL"/>
	</extensions>
	<properties>
		<if filter="isDeclarative">
			<bool name="Universal" value="true"/>
		</if>
		<bool name="UsesVGPUSymLink" value="true"/>
		<bool name="UsesWksServiceSymLink" value="true"/>
		<string name="WksServiceSymLinkPath" value="${{WksServiceSymLinkPath}}"/>
		<string name="VGPUSymLinkPath" value="${{vGPUSymLinkPath}}"/>
		<bool name="UsesNvFBCSymLink" value="true"/>
		<string name="NvFBCSymLinkPath" value="${{NvFBCSymLinkPath}}"/>
		<bool name="UsesNvProfileUpdaterSymLink" value="true"/>
		<string name="NvProfileUpdaterSymLinkPath" value="${{NvProfileUpdaterSymLinkPath}}"/>
		<bool name="UsesDisplayDriverRASSymLink" value="true"/>
		<string name="DisplayDriverRASSymLinkPath" value="${{DisplayDriverRASSymLinkPath}}"/>
		<bool name="FixedInstallLocation" value="true"/>
		<bool name="SkipCPLInstall"/>
		<string name="ArpDisplayName" value="${{arpDisplayName}}"/>
		<string name="DriverServiceName" value="${{driverServiceName}}"/>
		<bool name="UseDisplayContainer" value="true"/>
		<bool name="InstallDriverOnly" value="${{installDriverOnly}}"/>
		<bool name="InstallAppsOnly" value="${{installAppsOnly}}"/>
		<bool name="IsContainerDeclarative" value="${{isContainerDeclarative}}"/>
		<bool name="IsCPLDeclarative" value="${{isCPLDeclarative}}"/>
		<string name="PluginsAndSymLinks" value="${{vGPUSymLinkPath}};
            ${{NvFBCSymLinkPath}};
            ${{NvProfileUpdaterSymLinkPath}};
            ${{DisplayDriverRASSymLinkPath}};
            ${{NvToppsSymLinkPath}};
            ${{NvWatchDogSymlinkPath}};
            ${{WksServiceSymLinkPath}};
            ${{vGPUConfigPath}};
            ${{NvFBCFolderPath}};
            ${{NvToppsFolderPath}}\${{OptionalNvToppsPlugin}};
            ${{DisplayDriverRASPluginPath}};
            ${{NvProfileUpdaterDirPath}};
            ${{NvWatchDogFolderPath}};
            ${{NvCPLCorePluginPath}};
            ${{NvCPLSyncPluginPath}};
            ${{NvMessagebusbroadcastPlugin}};
            ${{WksServicePluginFolderPath}}\${{WksServicePlugin}};"/>
		<bool name="SameDriver" value="false"/>
